{
    "name": "bjorndcode/nova-analytics",
    "description": "Analytics tool for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "analytics"
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../nova"
        }
    ],
    "homepage": "https://github.com/BjornDCode/nova-analytics",
    "license": "MIT",
    "authors": [
        {
            "name": "Bjorn Lindholm",
            "email": "bjornlindholmhansen@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "7.1"
    },
    "autoload": {
        "psr-4": {
            "BjornDCode\\NovaAnalyticsTool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BjornDCode\\NovaAnalyticsTool\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BjornDCode\\NovaAnalyticsTool\\AnalyticsToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
